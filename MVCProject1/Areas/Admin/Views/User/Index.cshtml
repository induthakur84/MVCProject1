<br />
<div class="row">
    <div class="col-9">
        <h2 class="text-primary">User List</h2>
    </div>
    <div class="col-3">
        <a class="btn btn-info" asp-area="Identity" asp-page="/Account/Register">
            <i class="fas fa-plus"></i>
            Create New User
        </a>
    </div>
</div>
<div class="border p-2 m-2">
    <table id="tblData" class="table table-striped table-bordered">
        <thead>
            <tr class="table-hover">
                <th>Name</th>
                <th>Email</th>
                <th>Phone Number</th>
                <th>Company</th>
                <th>Role</th>
                <th></th>
            </tr>
        </thead>
    </table>

    @section scripts
        {
        <script src="~/js/user.js"></script>
    }
